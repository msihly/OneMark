{
	"name": "onemark",
	"version": "3.0.0",
	"author": "Mohamed Sihly",
	"private": true,
	"dependencies": {
		"@babel/plugin-proposal-object-rest-spread": "^7.13.8",
		"@babel/plugin-transform-spread": "^7.13.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-redux": "^7.2.2",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.1",
		"react-toastify": "^7.0.4",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0",
		"sass": "^1.30.0"
	},
	"scripts": {
		"dev": "concurrently \"npm run start\" && \"nodemon --inspect --trace-warnings ../server/index.js\"",
